<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/activity_base_title_bar" />

    <ScrollView
        android:id="@+id/scroll_order_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout style="@style/ll_match_content_ver">
           <View
               style="@style/line_1"
               android:layout_marginTop="@dimen/px_14"/>

            <RelativeLayout
                style="@style/rl_wrap_content"
                android:background="@color/white"
                android:padding="@dimen/px_20">

                <LinearLayout
                    style="@style/ll_wrap_content_hor"
                    android:layout_centerVertical="true">

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="订单" />

                    <TextView
                        android:id="@+id/tv_order_num_order_detail"
                        style="@style/grayTextView_333333_13"

                        android:text="123213325434534" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_success_order_detail"
                    style="@style/redTextView_c91014"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:textSize="13sp"
                    android:text="交易成功" />
            </RelativeLayout>

            <View style="@style/line_1" />

            <com.aglook.comapp.view.MyListView
                android:id="@+id/lv_all_order_lv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:divider="@null"
                ></com.aglook.comapp.view.MyListView>

            <View style="@style/line_1" />

            <LinearLayout
                style="@style/ll_match_wrap_content_ver"
                android:background="@color/gray_eeeeee"
                android:gravity="right|center_vertical"
                android:paddingLeft="@dimen/px_20"
                android:paddingRight="@dimen/px_20"
                android:paddingTop="@dimen/px_20">

                <LinearLayout
                    style="@style/ll_match_wrap_content_hor"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical">

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="共" />

                    <TextView
                        android:id="@+id/tv_order_total_order_detail"
                        style="@style/grayTextView_333333_13"
                        android:text="2" />

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:layout_marginRight="@dimen/px_10"
                        android:text="件商品" />

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="合计：" />

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="￥" />

                    <TextView
                        android:id="@+id/tv_money_order_detail"
                        style="@style/grayTextView_333333_13"
                        android:text="3333.33" />

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="（含手续费：￥" />

                    <TextView
                        android:id="@+id/tv_cost_order_detail"
                        style="@style/grayTextView_333333_13"
                        android:text="0.00" />

                    <TextView
                        style="@style/grayTextView_333333_13"
                        android:text="）" />
                </LinearLayout>

                <LinearLayout
                    style="@style/ll_wrap_content_hor"
                    android:layout_alignParentRight="true"
                    android:gravity="center_vertical"
                    android:layout_centerVertical="true">

                    <TextView
                        android:id="@+id/tv_delete_order_detail"
                        style="@style/redTextView_c91014"
                        android:layout_marginTop="@dimen/px_10"
                        android:background="@drawable/shanchu"
                        android:layout_marginBottom="@dimen/px_3"
                        android:gravity="center"
                        android:text="仓单交易"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/tv_click_all_order_lv"
                        style="@style/redTextView_c91014"
                        android:layout_marginTop="@dimen/px_10"
                        android:background="@drawable/red_back"
                        android:gravity="center"
                        android:text="提货"
                        android:textColor="@color/white"
                        android:layout_marginBottom="@dimen/px_3"
                        android:layout_marginLeft="@dimen/px_20"
                        android:textSize="13sp" />
                </LinearLayout>
            </LinearLayout>

           <View
              style="@style/line_1"
               android:layout_marginTop="@dimen/px_14"/>
            <LinearLayout
                style="@style/ll_match_content_ver"
                android:background="@color/white">
                <TextView
                   style="@style/grayTextView_333333_13"
                    android:paddingLeft="@dimen/px_20"
                    android:paddingTop="@dimen/px_10"
                    android:paddingBottom="@dimen/px_10"
                    android:text="订单详情"/>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                   style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="下单时间"/>
                    <TextView
                        android:id="@+id/tv_xia_order_detail"
                       style="@style/orderDetail_TextView_333333_13"
                        android:text="2015-11-11 16:22"/>
                   </LinearLayout>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="付款时间"/>
                    <TextView
                        android:id="@+id/tv_pay_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="2015-11-11 16:22"/>
                </LinearLayout>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="联系电话"/>
                    <TextView
                        android:id="@+id/tv_phone_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="15646879"/>
                </LinearLayout>
                <View
                    style="@style/line_1"/>
                <TextView
                    style="@style/grayTextView_333333_13"
                    android:paddingLeft="@dimen/px_20"
                    android:paddingTop="@dimen/px_10"
                    android:paddingBottom="@dimen/px_10"
                    android:text="收货信息"/>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="用户账号"/>
                    <TextView
                        android:id="@+id/tv_username_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="0295"/>
                </LinearLayout>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="收货姓名"/>
                    <TextView
                        android:id="@+id/tv_name_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="王大锤"/>
                </LinearLayout>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="收货地址"/>
                    <TextView
                        android:id="@+id/tv_address_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="大钟寺"/>
                </LinearLayout>
                <View
                    style="@style/line_1"/>
                <LinearLayout
                    style="@style/orderDetail_ll_weight">
                    <TextView
                        style="@style/orderDetail_TextView_666666_13"
                        android:text="完成时间"/>
                    <TextView
                        android:id="@+id/tv_over_order_detail"
                        style="@style/orderDetail_TextView_333333_13"
                        android:text="2011-11-11 16:22"/>
                </LinearLayout>
                </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
