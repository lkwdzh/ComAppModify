<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_eeeeee"
    android:orientation="vertical">

    <include layout="@layout/activity_base_title_bar" />

    <View
        style="@style/line_1"
        android:layout_marginTop="@dimen/px_14" />

    <LinearLayout style="@style/guadan_ll_weight">

        <TextView
            style="@style/guadan_TextView_333333"
            android:text="席位号" />

        <EditText
            android:id="@+id/et_seatNO_Friends_add"
            style="@style/guadan_edittext_weight"
            android:inputType="number"
            android:hint="请输入联系人席位号" />
    </LinearLayout>

    <View style="@style/line_1" />



</LinearLayout>
