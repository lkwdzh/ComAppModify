<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
<!-- LinearLayout与RelativeLa布局-->
    <style name="ll_match_content_hor">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_match_wrap_content_hor">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_wrap_match_content_hor">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_match_content_ver">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_match_wrap_content_ver">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_wrap_match_content_ver">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ll_wrap_content_hor">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_wrap_content_ver">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="rl_match_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="rl_wrap_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="rl_match_wrap_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="rl_wrap_match_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!--白色的textview-->
    <style name="whiteTextView" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--全横分割线-->
    <style name="line_1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/gray_line</item>
    </style>

    <!--竖分割线-->
    <style name="line_2">
        <item name="android:layout_width">0.5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/gray_line</item>
    </style>

    <!--radioButton背景-->
    <style name="radioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
    </style>

    <!--333333色的textview_18sp-->
    <style name="grayTextView_333333_18" parent="android:Widget.TextView">
        <item name="android:textColor">@color/textcolor_333333</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!--333333色的textview-->
    <style name="grayTextView_333333" parent="android:Widget.TextView">
        <item name="android:textColor">@color/textcolor_333333</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--666666色的textview_15sp-->
    <style name="grayTextView_666666_15" parent="android:Widget.TextView">
        <item name="android:textColor">@color/textcolor_666666</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
    </style>
    <!--666666色的textview-->
    <style name="grayTextView_666666" parent="android:Widget.TextView">
        <item name="android:textColor">@color/textcolor_666666</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--666666色的textview-->
    <style name="grayTextView_999999" parent="android:Widget.TextView">
        <item name="android:textColor">@color/textcolor_999999</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--红色c91014的textview_15sp-->
    <style name="redTextView_c91014" parent="android:Widget.TextView">
        <item name="android:textColor">@color/red_c91014</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!--红色f47070的textview_-->
    <style name="redTextView_f47070" parent="android:Widget.TextView">
        <item name="android:textColor">@color/red_f47070</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>

